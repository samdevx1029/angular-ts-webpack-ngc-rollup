{
  "name": "hamilton-plate-tracker-spa",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "postinstall": "cd src && typings install && cd ..",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "webpackbuild": "rimraf dist && cross-env NODE_ENV=production webpack --progress --profile",
    "rimrafdev": "rimraf index.html && rimraf globals.js",
    "webpackbuilddev": "npm run rimrafdev && webpack --config config/webpack.dev.js --progress --profile",
    "ngc": "rimraf src/aot && \"node_modules/.bin/ngc\" -p src/tsconfig-aot.json",
    "rollup": "rimraf dist/app.js && \"node_modules/.bin/rollup\" -c rollup.js",
    "rollupdev": "rimraf app.js && \"node_modules/.bin/rollup\" -c config/rollup.dev.js",
    "rollupbuild": "npm run ngc && npm run rollup",
    "rollupbuilddev": "npm run ngc && npm run rollupdev",
    "build": "npm run webpackbuild && npm run rollupbuild",
    "builddev": "npm run webpackbuilddev && npm run rollupbuilddev",
    "serve": "cd dist && lite-server",
    "lite": "lite-server",
    "servedev": "gulp",
    "minify": "uglifyjs ./dist/app.b6e3cff8f67d9d84a1b0.js --screw-ie8 --compress --mangle --output ./dist/app.b6e3cff8f67d9d84a1b0.min.js",
    "test": "karma start"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/forms": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/router": "3.0.1",

    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "0.6.3",
    "@angular/platform-server": "2.0.1",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "2.2.4",
    "child-process": "1.0.2",
    "concurrently": "2.2.0",
    "cross-env": "2.0.1",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-watch": "4.3.10",
    "gulp-batch": "1.0.5",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "jasmine-core": "2.5.2",
    "jshint": "2.9.3",
    "jshint-loader": "0.8.3",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "lite-server": "2.2.2",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.12",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "rollup": "^0.36.0",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "string-replace-loader": "1.0.4",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typings": "1.4.0",
    "uglifyjs": "2.4.10",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.25",
